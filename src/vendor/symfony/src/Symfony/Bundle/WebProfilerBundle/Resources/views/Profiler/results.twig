{% extends 'WebProfilerBundle:Profiler:layout.twig' %}

{% block content %}
    <h2>Search Results</h2>

    <table>
        <tr>
            <th>Token</th>
            <th>IP</th>
            <th>URL</th>
            <th>Time</th>
        </tr>
        {% for elements in tokens %}
            <tr>
                <td><a href="{{ path('_profiler', { 'token': elements.token }) }}">{{ elements.token }}</a></td>
                <td>{{ elements.ip }}</td>
                <td>{{ elements.url }}</td>
                <td>{{ elements.time|date('r') }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
